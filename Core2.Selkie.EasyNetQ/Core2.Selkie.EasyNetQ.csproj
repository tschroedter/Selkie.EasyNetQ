<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageIconUrl>https://nuget.org/Content/Images/packageDefaultIcon-50x50.png</PackageIconUrl>
    <PackageProjectUrl>https://github.com/tschroedter/Selkie.EasyNetQ</PackageProjectUrl>
    <RepositoryUrl>https://github.com/tschroedter/Selkie.EasyNetQ/tree/netcore2</RepositoryUrl>
    <PackageLicenseUrl>https://github.com/tschroedter/Selkie.EasyNetQ/blob/master/LICENSE</PackageLicenseUrl>
    <PackageTags>Selkie, EasyNetQ, Core2</PackageTags>
    <Copyright>Copyright Â© Thomas Schroedter 2017</Copyright>
    <Description>.Net Core 2: Some small extensions and installers for EasyNetQ. The package contains a extension to subscribe to messages async and a logger.</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Core2.EasyNetQ.Management.Client" Version="1.0.1" />
    <PackageReference Include="Core2.Selkie.Windsor" Version="1.0.1" />
    <PackageReference Include="EasyNetQ" Version="2.2.0" />
  </ItemGroup>

</Project>
